                                <tr>
                                    <td colspan="2" class="table-header"><strong>Fecha: </strong>{{summary.date_str}}</td>
                                </tr>
                                <tr ng-repeat="element in summary.summaryElements">
                                    <td>
                                        {{element.measurement.name}}
                                    </td>
                                    <td>
